<div class="panel panel-default" id="prev_cover_panel">
	<div class="panel-body">
		<% if(cover) { %>
			<div class="cover_panel" style="background-image: url('/uploads/cover/<%= cover.main_image %>');" ng-init="imageUrl = '<%= cover.main_image %>'" ng-class="{'grid': showGrid}">
		<% } else { %>
			<div class="cover_panel" style="background-color: #444;" ng-class="{'grid': showGrid}">
		<% } %>



			<div ng-repeat="(i, w) in widgets"  ng-click="setActiveWidget(i)">
				<div class="cover_panel_w w_text"  ng-if="w.name == 'donationalerts_donation_goal'" ng-style="{'top': w.posY, 'left': w.posX}" ng-class="{'active rails outline': showOpt == i}" my-draggable>
					<div class="donatalerts_goal_title" style="width: 600px; transform: translate(-50%, 0px); text-align: center;" ng-style="progressBarText1(w, {'left': w.barWidth / 2, 'top': w.textSize * 1.5 * -1})">
						<span style="position: static;" ng-if="w.titleShow == 'import'">Заголовок из DonationAlerts</span>
						<span style="position: static;" ng-if="w.titleShow == 'my'">{{w.myTitle}}</span>
					</div>

					<div class="donatalerts_goal_bar" ng-style="{'width': w.barWidth, 'height': w.barHeight, 'background': w.barBg, 'border-radius': w.barRadius+'px'}">
						<div class="donatalerts_goal_progressbar" ng-style="{'width': '50%', 'height': w.barHeight, 'background': w.barProgressColor, 'border-top-left-radius': w.barRadius+'px', 'border-bottom-left-radius': w.barRadius+'px'}"></div>
						<div class="donatalerts_goal_text_in_bar" ng-if="w.showProgressText === true" ng-style="progressBarText2(w, {'width': '100%', 'text-align': 'center', 'line-height': w.barHeight+'px'})">3 000 RUB (50%)</div>
						
						<div class="donatalerts_goal_counts" ng-if="w.showGoalBorder === true" ng-style="progressBarText1(w, {'width': '100%', 'bottom': w.textSize * 1.15 * -1}, true)">
							<span style="float: left; text-align: left; position: static;">0</span>
							<span style="float: right; text-align: right; position: static;">6 000</span>
						</div>

						<div class="donatalerts_goal_end_date" ng-if="w.showToDate === true" ng-style="progressBarText1(w, {'width': '100%', 'bottom': w.textSize * 1.15 * -1, 'text-align': 'center'}, true)">
							Осталось 5 дней
						</div>
					</div>
				</div>


				<div class="cover_panel_w w_text"  ng-if="w.name == 'donationalerts_instream_stats'" ng-style="{'top': w.posY, 'left': w.posX}" ng-class="{'active': showOpt == i}" my-draggable>
					<div class="donationalerts_instream_stats" ng-style="donationalerts_instream_stats_styles(w)" ng-if="w.viewType == 'list'">
						<span style="position: static; display: block;" ng-repeat="val in range(1, w.listCount)" ng-style="{'margin-bottom': w.listPadding}">{{w.listTextTemplate}}</span>
					</div>

					<div ng-if="w.viewType == 'slider'">
						<div class="donationalerts_instream_stats">
							<span style="position: static; display: block;" ng-style="donationalerts_instream_stats_styles(w)">{{w.sliderTextTemplate}}</span>
							<span style="position: static; display: block;" ng-style="donationalerts_instream_stats_styles_2(w)" ng-bind-html="donationalerts_instream_stats_lorem(w)" ng-if="w.sliderShowSecondText == true"></span>
						</div>
					</div>
				</div>

				
				<div class="cover_panel_w w_text"  ng-if="w.name == 'image_to_link'" ng-style="{'top': w.posY, 'left': w.posX}" ng-class="{'active rails outline': showOpt == i}" my-draggable>
					<img class="" ng-src="{{w.url}}" ng-style="{'width': w.width}" style="display: block; position: static;">
				</div>


				<div class="cover_panel_w w_text"  ng-if="w.name == 'online'" ng-style="textStyle(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;">
						{{w.text.before}}
						<span ng-if="w.delimiter == '0'" style="display: inline; position: static;">2521</span>
						<span ng-if="w.delimiter == '1'" style="display: inline; position: static;">2 521</span>
						<span ng-if="w.delimiter == '2'" style="display: inline; position: static;">2.521</span>
						<span ng-if="w.delimiter == '3'" style="display: inline; position: static;">2,521</span>
						{{w.text.after}}</span>
				</div>

				
				<div class="cover_panel_w w_text"  ng-if="w.name == 'random_text'" ng-style="textStyle(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;" ng-bind-html="newlines(w.text)"></span>
				</div>


				<div class="cover_panel_w w_text"  ng-if="w.name == 'text'" ng-style="textStyle(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;" ng-repeat="text in w.text">{{text.val}}</span>
				</div>

				<div class="cover_panel_w w_text w_rss"  ng-if="w.name == 'rss'" ng-style="textStyle(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<div ng-init="test_rss = [
						'В Казани выходит в прокат фильм о татарской Золушке',
						'Кинорецензия: как Роланд не дошел до Темной башни',
						'Москва: Как российские фильмы покоряли Венецианский кинофестиваль',
						'Новая серия «Игры престолов» опубликована в сети раньше выхода в эфир',
						'В Петербурге состоится показ картины о 80 чудесах света'
					];"></div>
					<style>
						.w_rss span:last-child {margin-bottom: 0 !important;}
					</style>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;" ng-style="{'margin-bottom': w.padding}" ng-repeat="(i, text) in test_rss | limitTo: w.count">
						<span style="position: static;" ng-if="w.showTime == true">1{{i}}:{{i}}{{i}} </span>{{textLimit(text, w.symbolLimit)}}
					</span>
				</div>

				<div class="cover_panel_w w_text"  ng-if="w.name == 'text_link'" ng-style="cover_panel.style(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;">
						{{w.textBefore}} [ваши данные] {{w.textAfter}}
					</span>
				</div>

				<div class="cover_panel_w w_text"  ng-if="w.name == 'members_count'" ng-style="cover_panel.style(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;">
						{{w.textBefore}} 
						<span ng-if="w.delimiter == '0'" style="display: inline; position: static;">23521</span>
						<span ng-if="w.delimiter == '1'" style="display: inline; position: static;">23 521</span>
						<span ng-if="w.delimiter == '2'" style="display: inline; position: static;">23.521</span>
						<span ng-if="w.delimiter == '3'" style="display: inline; position: static;">23,521</span>
						 {{w.textAfter}}
					</span>
				</div>


				<div class="cover_panel_w w_text"  ng-if="w.name == 'youtube_last_video' || w.name == 'youtube_video_to_link'" ng-style="{'top': w.posY, 'left': w.posX}" style="font-family: Arial;" ng-class="{'active': showOpt == i}" my-draggable>
					<img src="/img/youtube_thumb.png" ng-style="{'width': w.imgWidth, 'height': w.imgWidth / 16 * 9}" style="display: block; position: static;">

					<div ng-if="w.styleWidget == '3' && w.show_title"
						style="white-space: nowrap;"
						ng-style="{'top': w.title_offsetY,
									  'left': w.imgWidth + w.likes_offset,
									  'font-size': w.title_fontSize,
									  'color': w.title_color
									}"
						ng-class="{'bold': w.title_bold, 'caps': w.title_caps}"
						ng-bind-html="formatYoutubeTitle(w)">
					</div>

					<div ng-if="w.styleWidget == '3'"
						  style="white-space: nowrap;"
						  ng-style="{
							  'top': w.likes_offsetY,
							  'left': w.imgWidth + w.likes_offset,
							  'font-size': w.likes_fontSize,
							  'color': w.likes_color
							}">
						<div style="position: static;" ng-if="w.show_likes">
							<i style="position: static;" class="fa fa-thumbs-up"></i> 312 тыс.
						</div>
						<div style="position: static;" ng-if="w.show_dislikes">
							<i style="position: static;" class="fa fa-thumbs-down"></i> 998
						</div>
						<div style="position: static;" ng-if="w.show_view">
							<i style="position: static;" class="fa fa-eye"></i> 1 млн
						</div>
						<div style="position: static;" ng-if="w.show_comments">
							<i style="position: static;" class="fa fa-comment"></i> 500 тыс.
						</div>
					</div>

					<div ng-if="w.styleWidget == '1' || w.styleWidget == '2'" style="position: static;">
						<span ng-if="w.show_likes_bar == true && w.styleWidget == '1'" class="like-bar" style="
							display: block;
							position: static;
							left: -1px;
							width: 100%;
							height: 4px;
							background: #ddd;
						">
							<span style="
								width: 80%;
								height: 4px;
								background: #2693e6;
							"></span>
						</span>
						<div ng-if="w.show_likes == true" ng-style="{'font-size': w.likes_fontSize, 'color': w.likes_color, 'top': w.likes_offset + (w.imgWidth / 16 * 9), 'left': 0}">
							<i style="position: static;" class="fa fa-thumbs-up"></i> 800
						</div>
						<div ng-if="w.show_dislikes == true && w.styleWidget == '1'" ng-style="{'font-size': w.likes_fontSize, 'color': w.likes_color, 'top': w.likes_offset + (w.imgWidth / 16 * 9), 'right': 0}">
							<i style="position: static;" class="fa fa-thumbs-down"></i> 200<br>
						</div>
						<div ng-if="w.show_view == true && w.styleWidget == '2'" ng-style="{'font-size': w.likes_fontSize, 'color': w.likes_color, 'top': w.likes_offset + (w.imgWidth / 16 * 9), 'right': 0}">
							<i style="position: static;" class="fa fa-eye"></i> 20 тыс.<br>
						</div>
					</div>
				</div>


				<div class="cover_panel_w w_text"  ng-if="w.name == 'youtube_channel_info'" ng-style="{'top': w.posY, 'left': w.posX}" style="font-family: Arial;" ng-class="{'active': showOpt == i}" my-draggable>
					<img src="/img/yotube_channel_icon.jpg" ng-style="{'width': w.image_width, 'height': w.image_width, 'border-radius': w.image_radius+'%'}" style="display: block; position: static;">

					<div style="white-space: nowrap;"
						  ng-style="youtube_channel_info_pos(w)">
						<div style="position: static;"
							  ng-if="w.title_show"
							  ng-class="{'bold': w.title_bold, 'caps': w.title_caps}"
							  ng-style="{'color': w.title_color, 'font-size': w.title_fontSize, 'margin-bottom': w.title_paddingY}">
							Название вашего канала</div>

						<div style="position: static;" ng-class="{'bold': w.text_bold}" ng-if="w.subs_show">
							<i style="position: static;" class="fa fa-users" ng-if="w.show_icons"></i>
							<span style="position: static;" ng-if="!w.show_icons">Подписчики:</span>
							 1 млн
						</div>
						<div style="position: static;" ng-class="{'bold': w.text_bold}" ng-if="w.view_show">
							<i style="position: static;" class="fa fa-eye" ng-if="w.show_icons"></i>
							<span style="position: static;" ng-if="!w.show_icons">Просмотры:</span>
							 15 млн
						</div>
						<div style="position: static;" ng-class="{'bold': w.text_bold}" ng-if="w.videos_show">
							<i style="position: static;" class="fa fa-film" ng-if="w.show_icons"></i>
							<span style="position: static;" ng-if="!w.show_icons">Видео:</span>
							 35
						</div>
					</div>
				</div>


				

				<div class="cover_panel_w w_text"  ng-if="w.name == 'rates'" ng-style="textStyle(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;">
						<i class="fa fa-rub" style="position: static; line-height: 1.2;" ng-if="w.perfix == 'icon' && w.firstCurrency == 'RUB'"></i>
						<i class="fa fa-dollar" style="position: static; line-height: 1.2;" ng-if="w.perfix == 'icon' && w.firstCurrency == 'USD'"></i>
						<i class="fa fa-euro" style="position: static; line-height: 1.2;" ng-if="w.perfix == 'icon' && w.firstCurrency == 'EUR'"></i>
						<i class="fa fa-btc" style="position: static; line-height: 1.2;" ng-if="w.perfix == 'icon' && w.firstCurrency == 'BTC'"></i>
						<i class="fa fa-angle-double-right" style="position: static; line-height: 1.2;" 
							ng-if="w.perfix == 'icon' && (w.firstCurrency != 'RUB' && w.firstCurrency != 'USD' && w.firstCurrency != 'EUR' && w.firstCurrency != 'BTC')"></i>
						<span style="position: static;" ng-if="w.perfix == 'text_code'">{{w.firstCurrency}}</span>
						 60.32
					</span>
				</div>

				<div class="cover_panel_w w_text"  ng-if="w.name == 'time'" ng-style="cover_panel.style(w)" ng-class="{'active': showOpt == i}" my-draggable>
					<span ng-class="{'bold': w.font_bold, 'caps': w.caps}" style="display: block; position: static;">{{w.text}} {{showFormatedTime(w.timezone, w.dateFormat)}} {{w.textEnd}}</span>
				</div>

				<div class="cover_panel_w w_text w_timer" ng-class="{'bold': w.font_bold, 'caps': w.caps, 'active': showOpt == i}" ng-if="w.name == 'timer'" ng-style="timerStyle(w)" my-draggable>
					<div class="days" ng-style="timerIntStyle(w, 'd')">
						<span style="position: static;" ng-if="w.hideZero != true">0</span>2<div style="position: static;" ng-style="timerLabelStyle(w)">Дней</div>
					</div>
					
					<div class="hour" ng-style="timerIntStyle(w, 'h')">
						17<div style="position: static;" ng-style="timerLabelStyle(w)">Часов</div>
					</div>
					
					<div class="mins" ng-style="timerIntStyle(w, 'm')">
						<span style="position: static;" ng-if="w.hideZero != true">0</span>5<div style="position: static;" ng-style="timerLabelStyle(w)">Минут</div>
					</div>
				</div>

				<div class="cover_panel_w w_text"  ng-if="w.name == 'weather'" ng-style="cover_panel.style(w)" ng-class="{'bold': w.font_bold, 'caps': w.caps, 'active': showOpt == i}" my-draggable>
					<span style="display: block; position: static;">{{w.before_text}} <span style="position: relative;" ng-if="!w.only_temp">солнечно</span> 27°<span style="position: relative;" ng-if="w.show_c">C</span></span>
				</div>

				<div class="cover_panel_w w_text w_weather_new"  ng-if="w.name == 'weather_icon'" ng-style="cover_panel.style(w)" ng-class="{'bold': w.font_bold, 'caps': w.caps, 'active': showOpt == i}" my-draggable>
					<div ng-if="w.iconShow" ng-style="{'top': w.iconPosY, 'left': w.iconPosX}" style="transform: translate(-50%, -50%);">
						<img ng-src="/img/weather_icons/{{w.iconPack}}/02d.png" ng-style="{'width': w.iconSize}" style="display: block; position: static;">
					</div>
					<span style="display: block; position: relative;">27°<span style="position: relative;" ng-if="w.show_c">C</span></span>
					<div ng-if="w.textShow" ng-style="{'top': w.textPosY, 'left': w.textPosX, 'font-size': w.textSize}">облачно</div>
				</div>



				<div class="cover_panel_w w_text w_weather_new"  ng-if="w.name == 'probki'" ng-style="cover_panel.style(w)" ng-class="{'bold': w.font_bold, 'caps': w.caps, 'active': showOpt == i}" my-draggable>
					<div ng-if="w.iconShow" ng-style="{'top': w.iconPosY, 'left': w.iconPosX}" style="transform: translate(-50%, -50%);">
						<img ng-src="/img/probki_icons/{{w.iconPack}}/yellow.png" ng-style="{'width': w.iconSize}" style="display: block; position: static;">
					</div>
					<span style="display: block; position: relative;">3 <span style="position: relative;" ng-if="w.show_bale_text">балла</span></span>
					<div ng-if="w.textShow" style="width: auto; white-space: nowrap;" ng-style="{'top': w.textPosY, 'left': w.textPosX, 'font-size': w.textSize}">Местами затруднения</div>
				</div>



				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'birthday'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)" my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
				</div>

				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'show_user_to_link'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)" my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
				</div>

				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'last_sub'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)" my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
				</div>


				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'best_comment'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)"
						my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
					<div class="icon" ng-if="w.iconShow" ng-style="styleSmallElem(w, 'icon')" style="line-height: 14px;" my-draggable><i class="fa fa-tw fa-comment" style="line-height: 1; position: static;"></i></div>
					<div class="count" ng-if="w.countShow"  ng-style="styleSmallElem(w, 'count')" my-draggable>40</div>
				</div>


				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'best_like'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)"
						my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
					<div class="icon" ng-if="w.iconShow" ng-style="styleSmallElem(w, 'icon')" style="line-height: 14px;" my-draggable><i class="fa fa-tw fa-heart" style="line-height: 1; position: static;"></i></div>
					<div class="count" ng-if="w.countShow"  ng-style="styleSmallElem(w, 'count')" my-draggable>23</div>
				</div>


				<div class="cover_panel_w w_last_sub" ng-if="w.name == 'best_share'" ng-class="{'active': showOpt == i}">
					<div class="avatar" ng-style="{'top': w.posY, 'left': w.posX}" my-draggable>
						<img src="/img/user_100.jpg" ng-style="avatarStyle(w)">
					</div>
					<div class="name pos-{{w.namePos}} format-{{w.nameFormat}}" 
						ng-class="{'bold': w.font_bold, 'caps': w.caps}" 
						ng-style="styleLastSubName(w)"
						my-draggable>
						<span class="first-name">Александр </span>
						<span class="last-name">Мухаметзянов</span>
					</div>
					<div class="icon" ng-if="w.iconShow" ng-style="styleSmallElem(w, 'icon')" style="line-height: 14px;" my-draggable><i class="fa fa-tw fa-bullhorn" style="line-height: 1; position: static;"></i></div>
					<div class="count" ng-if="w.countShow"  ng-style="styleSmallElem(w, 'count')" my-draggable>15</div>
				</div>


			
				
			</div>
		</div>
	</div>
</div>


<div class="panel panel-default">
	<div class="panel-heading clearfix">
		<b style="padding-top: 5px; display: inline-block; font-size: 15px;">Настройки виджета</b>
		<button type="button" class="btn btn-sm btn-success" style="float: right;" ng-click="widgets_modal()"><i class="fa fa-tw fa-plus"></i> Добавить виджет</button>
